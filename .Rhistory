#print(pat)
if(os == 0){
row = 1
md.db_0 = c(md.db_0, round(as.double(as.character(df.results[pat,"MD.db"])), 2))
}
else if(inside.range(os, c(1,3))){
row = 2
md.db_13 = c(md.db_13, round(as.double(as.character(df.results[pat,"MD.db"])), 2))
}
else if(inside.range(os, c(4,5))){
row = 3
